{
    "name": "jovialcore/cake-cloudinary",
    "description": "CakeCloudinary plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Chidiebere Chukwudi",
            "email": "chidideveloper@gmail.com",
            "homepage": "https://github.com/jovialcore"
        }
    ],
    "homepage": "https://github.com/jovialcore/cake-cloudinary",
    "keywords": [
        "cakephp",
        "cloudinary-php",
        "File Uploads",
        "php",
        "Cloudinary"
    ],
    "require": {
        "php": ">=7.2",
        "cakephp/cakephp": "^4.3",
        "cloudinary/cloudinary_php": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5 || ^9.3"
    },
    "autoload": {
        "psr-4": {
            "CakeCloudinary\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CakeCloudinary\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
